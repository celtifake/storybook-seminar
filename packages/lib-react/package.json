{
  "name": "lib-react",
  "private": true,
  "version": "0.0.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:scripts": "vite build",
    "build:types": "tsc --emitDeclarationOnly && tsc-alias",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "tsc-alias": "^1.6.7",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
